#include <iostream>
using namespace std;
int fib(int n)
{
    if(n==0 or n==1)return 1;//base case 1 1 2 3 5 here first two are ones so return 1
    
    return fib(n-2)+fib(n-1);//to compute fib(n)=fib(n-1)+fib(n-2)
    
}
int main() {
    int n;
    cin>>n;
    cout<<fib(n);
	//cout<<"GfG!";
	return 0;
}
